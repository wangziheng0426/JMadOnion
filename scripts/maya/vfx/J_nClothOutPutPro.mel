for ($temp=0;size($my_file)>0 && $temp<80000 ; $temp++)
    {
        $my_file=`fgetword $fileIdOfnCloth`;
        
    if ($my_file=="nClothNode")//判定为布料节点
        {$nClothCount++;
            $my_file=`fgetword $fileIdOfnCloth`;
            $nClothNodeName=($sel_nameSpace+$my_file);
            $my_file=`fgetword $fileIdOfnCloth`;
            $nClothMeshName=($sel_nameSpace+$my_file);
