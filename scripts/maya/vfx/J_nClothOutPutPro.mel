                {
                    modelEditor -e -nRigids 1 $currentPanel;
                }
                else if (`isTrue "MayaCreatorExists"` && `scriptedPanel -exists $currentPanel` 
                        && `scriptedPanel -query -type $currentPanel` == "dynPaintScriptedPanelType") 
